{
  "name": "multi-routing-api",
  "version": "2.8.0",
  "description": "redux api middleware for separate your api routes, api actions, and interceptors. provide state normalization, db reducer",
  "main": "lib/index.js",
  "scripts": {
    "cleanup": "npm prune && npm i && npm dedupe",
    "test": "node_modules/.bin/eslint --color --debug --config ./.eslintrc.json lib/**/*.js && node_modules/.bin/jest --coverage",
    "jest": "node_modules/.bin/jest --coverage",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliaksandr-master/multi-routing-api.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "fetch",
    "api",
    "routes",
    "action",
    "actions",
    "server",
    "normalizr",
    "normalization",
    "state",
    "client",
    "db"
  ],
  "author": "alxe.master@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliaksandr-master/multi-routing-api/issues"
  },
  "homepage": "https://github.com/aliaksandr-master/multi-routing-api#readme",
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.20.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-eslint": "3.0.0",
    "eslint-config-standard": "6.2.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-require-path-exists": "1.1.5",
    "eslint-plugin-standard": "2.0.1",
    "husky": "0.12.0",
    "jest": "18.1.0",
    "jest-cli": "18.1.0",
    "lodash": "^4.17.15",
    "webpack": "1.14.0",
    "whatwg-fetch": "2.0.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.4"
  },
  "dependencies": {
    "localforage": "1.4.3",
    "normalizr": "3.1.0",
    "urijs": "1.19.7"
  },
  "jest": {
    "automock": false,
    "notify": true,
    "testEnvironment": "jsdom",
    "cacheDirectory": ".tmp/jest-cache",
    "coverageDirectory": ".tmp/tests-coverage-report",
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": false
  }
}
